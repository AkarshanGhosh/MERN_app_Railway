{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "build":[
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {"src":"/(.*)", "dest": "index.js"}
    ]
}